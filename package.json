{
	"name": "staituned-website",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev:only": "vite dev --host --port 3000",
		"build:only": "CF_PAGES=1 vite build",
		"package": "vite package",
		"preview": "vite preview --host --port 4000",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"tailwind:watch": "TAILWIND_MODE=watch NODE_ENV=development tailwindcss -c tailwind.config.cjs -i src/styles/tailwind.css -o src/styles/tailwind-output.css --watch",
		"tailwind:build": "TAILWIND_MODE=build NODE_ENV=production tailwindcss -c tailwind.config.cjs -m -i src/styles/tailwind.css -o src/styles/tailwind-output.css",
		"dev": "concurrently \"npm run dev:only\" \"npm run tailwind:watch\"",
		"build": "npm run tailwind:build && npm run build:only"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.0",
		"@rollup/plugin-typescript": "^8.0.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/line-clamp": "^0.4.0",
		"@tailwindcss/typography": "^0.5.2",
		"@tsconfig/svelte": "^2.0.0",
		"@types/js-yaml": "^4.0.5",
		"@types/marked": "^4.0.3",
		"@types/node": "^18.7.18",
		"autoprefixer": "^10.4.12",
		"concurrently": "^7.2.2",
		"front-matter": "^4.0.2",
		"js-yaml": "^4.1.0",
		"mdsvex": "^0.10.6",
		"mdsvex-relative-images": "^1.0.2",
		"octokit": "^2.0.7",
		"postcss": "^8.4.16",
		"rehype-add-classes": "^1.0.0",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-external-links": "^2.0.1",
		"rehype-figure": "^1.0.1",
		"rehype-highlight": "^5.0.2",
		"rehype-katex-svelte": "^1.1.2",
		"rehype-mathjax": "^4.0.2",
		"rehype-slug": "^5.0.1",
		"rehype-stringify": "^9.0.3",
		"remark-frontmatter": "^4.0.1",
		"remark-gfm": "^3.0.1",
		"remark-math": "^3.0.0",
		"remark-parse": "^10.0.1",
		"remark-rehype": "^10.1.0",
		"rollup": "^2.3.4",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-livereload": "^2.0.0",
		"rollup-plugin-svelte": "^7.0.0",
		"rollup-plugin-terser": "^7.0.0",
		"sass": "^1.54.5",
		"svelte": "^3.0.0",
		"svelte-better-paginate": "^0.0.1",
		"svelte-check": "^2.0.0",
		"svelte-materialify": "^0.3.11",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.4",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0",
		"unified": "^10.1.2",
		"unist-util-visit": "^4.1.1",
		"dayjs": "^1.11.5"
	}
}
